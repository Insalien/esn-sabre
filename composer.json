{
    "name": "linagora/esn-sabre",
    "version": "0.1.0",
    "description": "SabreDAV Backend for Linagora ESN",
    "keywords": ["WebDAV", "CalDAV", "CardDAV", "iCalendar"],
    "license" : "AGPL-3.0",
    "authors": [{
            "name": "Linagora",
            "homepage" : "http://linagora.com"
        },{
            "name": "Philipp Kewisch",
            "email": "pkewisch@linagora.com",
            "role" : "Initiator"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/linagora/sabre-vobject"
        },
        {
            "type": "git",
            "url": "https://github.com/linagora/sabre-dav"
        }
    ],
    "require": {
        "php-amqplib/php-amqplib": "2.6.*",
        "php": ">=5.4.1",
        "sabre/dav": "dev-3.2.0-linagora",
        "sabre/vobject": "dev-358_bugfix_exception_process_message_reply as 4.1.0",
        "ext-mongo": "~1.5",
        "predis/predis": "1.0.3"
    },
    "require-dev" : {
        "phpunit/phpunit" : "~4.8",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4" : {
            "ESN\\DAV\\"        : "lib/DAV/",
            "ESN\\CalDAV\\"     : "lib/CalDAV/",
            "ESN\\CardDAV\\"    : "lib/CardDAV/",
            "ESN\\DAVACL\\"     : "lib/DAVACL/",
            "ESN\\JSON\\"       : "lib/JSON/",
            "ESN\\Utils\\"      : "lib/Utils/"
        }
    },
    "minimum-stability": "dev"
}
