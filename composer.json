{
    "name": "linagora/esn-sabre",
    "version": "0.1.0",
    "description": "SabreDAV Backend for Linagora ESN",
    "keywords": ["WebDAV", "CalDAV", "CardDAV", "iCalendar"],
    "license" : "AGPL-3.0",
    "authors": [{
            "name": "Linagora",
            "homepage" : "http://linagora.com"
        },{
            "name": "Philipp Kewisch",
            "email": "pkewisch@linagora.com",
            "role" : "Initiator"
        }
    ],
    "require": {
        "php": ">=5.4.1",
        "sabre/dav": "3.1.0",
        "sabre/vobject": "dev-master#d7727226da55cabd03eb4c630c6f3631873a7bf8",
        "ext-mongo": "~1.5",
        "predis/predis": "1.0.3"
    },
    "require-dev" : {
        "phpunit/phpunit" : "~4.8",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4" : {
            "ESN\\DAV\\"        : "lib/DAV/",
            "ESN\\CalDAV\\"     : "lib/CalDAV/",
            "ESN\\CardDAV\\"    : "lib/CardDAV/",
            "ESN\\DAVACL\\"     : "lib/DAVACL/",
            "ESN\\JSON\\"       : "lib/JSON/",
            "ESN\\Utils\\"      : "lib/Utils/"
        }
    },
    "minimum-stability": "dev"
}
